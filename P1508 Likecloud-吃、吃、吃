#include <bits/stdc++.h>
using namespace std;

int graph[209][209];
int f[209][209];

int main(){
    int m, n;
    scanf("%d %d", &m, &n);
    for(int i = 0;i < m;++i)
        for(int j = 0;j < n;++j)
            scanf("%d", &graph[i][j]);
    for(int i = m;i > 0;--i){
        for(int j = 0;j < n;++j){
            if((n + 1) / 2 + i - m <= j && j <= (n + 1) / 2 + m - i){
                if(j - 1 >= 0 && f[i - 1][j - 1] < f[i][j] + graph[i - 1][j - 1])
                    f[i - 1][j - 1] = f[i][j] + graph[i - 1][j - 1];
                if(f[i - 1][j] < f[i][j] + graph[i - 1][j])
                    f[i - 1][j] = f[i][j] + graph[i - 1][j];
                if(j + 1 < n && f[i - 1][j + 1] < f[i][j] + graph[i - 1][j + 1])
                    f[i - 1][j + 1] = f[i][j] + graph[i - 1][j + 1];
            }
        }
    }
    int sum = 0xb0000000;
    for(int i = (n + 1) / 2 - m;i <= (n + 1) / 2 + m;++i){
        if(sum < f[0][i]) sum = f[0][i];
    }
    printf("%d\n", sum);
    return 0;
}
