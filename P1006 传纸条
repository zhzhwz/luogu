#include <cstdio>
#include <cstring>
#include <algorithm>
using namespace std;

int graph[59][59];
int f[59][59][59];

int main(){
    int m, n;
    scanf("%d %d", &m, &n);
    for(int i = 0;i < m;++i){
        for(int j = 0;j < n;++j){
            scanf("%d", &graph[i][j]);
        }
    }
    memset(f, 0, sizeof(f));
    for(int i = 0;i < m;++i){
        for(int j = 0;j < n;++j){
            for(int k = i + 1;k < m;++k){
                if(i + j - k < 0) break;
                int s = 0;
                if(i > 0) s = max(s, f[i - 1][j][k - 1]);
                if(j > 0) s = max(s, f[i][j - 1][k]);
                if(i > 0 && i - 1 != k) s = max(s, f[i - 1][j][k]);
                if(j > 0 && i + 1 != k) s = max(s, f[i][j - 1][k - 1]);
                f[i][j][k] = s + graph[i][j] + graph[k][i + j - k];
            }
        }
    }
    printf("%d\n", f[n - 2][m - 1][n - 1]);
    return 0;
}
